class Solution {
    public int solution(int[] A) {
        // write your code in C# 6.0 with .NET 4.5 (Mono)
        int cnt =0;
        long[] temp = Array.ConvertAll(A,new Converter<int, long>(intToLong));
        //long[] temp= Array.ConvertAll(A, val => (long)val);

        Array.Sort(temp);
        for(int i=0; i<temp.Length;i++){
            if(temp.Length == 1 || i == temp.Length-1)
                return (int)temp[i];
            if(temp[i]==temp[i+1]){
                cnt++;
            }else{
                if(cnt==0)
                    return (int)temp[i];
                cnt =0;
            }    
        }
        return -1;
    }
    
    public long intToLong(int t){
        return (long)t;    
    }
}

